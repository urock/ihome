    121          uint8_t ow_read_bit (void)
    122          {
    123              uint8_t ret = 10;
    124          
    125              Set_Port_output ();
    126              Set_Port_Down ();
    127              Delay_US (5);
    128              Set_Port_input ();
    129              Delay_US (10);
    130              if (GPIOC->IDR & GPIO_Pin_7)
    131                  {
    132                      ret = 1;
    133                  }
    134              else
    135                  {
    136                      ret = 0;
    137                  }
    138              Delay_US (44);
    139              return ret; 
    140          }
    141          
    142          uint8_t ow_read_byte (void)
    143          {
    144              uint8_t i;
    145              uint8_t byte=0;
    146          
    147              for (i=0;i<8;i++)
    148                  {
    149                      if(ow_read_bit ()) byte |= (0x01<<i);
    150                      Delay_US (1);
    151                  }
    152              return(byte);
    153          }
    154          
    155          void ow_write_byte (uint8_t byte)
    156          {
    157              uint8_t a;
    158              uint8_t temp;
    159          
    160              for(a=0;a<8;a++)
    161                  {
    162                      temp = byte >> a;
    163                      temp &=0x01;
    164                      ow_write_bit (temp);
    165                      Delay_US (1);
    166                  }
    167          }
