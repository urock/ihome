    102          void ow_write_bit (uint8_t bit)
    103          {
    104              if (bit)
    105                  {
    106                      Set_Port_output ();
    107                      Set_Port_Down ();
    108                      Delay_US (10);
    109                      Set_Port_Up ();
    110                      Delay_US (49);
    111                  }
    112              else
    113                  {
    114                      Set_Port_output ();
    115                      Set_Port_Down ();
    116                      Delay_US (59);
    117                      Set_Port_Up ();
    118                  } 
    119          }
