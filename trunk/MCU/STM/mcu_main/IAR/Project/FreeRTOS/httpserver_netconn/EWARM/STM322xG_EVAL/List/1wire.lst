     89              Delay_US(80);
     90              if(GPIOC->IDR & GPIO_Pin_7)
     91                  {
     92                      ret = 0;
     93                  }
     94              else
     95                  {
     96                      ret = 1;
     97                  }
     98              Delay_US (400);
     99              return ret;
    100          }
