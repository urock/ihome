    401          void vWather_sensor (void *dev)
    402          {  
    403            int a;     // Расчет влажности
    404            uint8_t b; // Оценка влажности
    405            
    406            ADC_SoftwareStartConv(ADC1);
    407            
    408            for (;;)
    409            {
    410                a = ((3300 * ADC1ConvertedValue[0]) / 4095); //Wather
    411                
    412                if ((0 < a)&&(a < 300)){
    413                  b = 1;  // Мокро
    414                }
    415                
    416                if ((301 < a)&&(a < 700)){
    417                  b = 2;  // Влажно
    418                }
    419                
    420                if (701 < a){
    421                  b = 3;   //Сухо
    422                }
    423                
    424                if (ADC1ConvertedValue[1] > 100){  //GAS
    425                 
    426                  LEDOn(LED4); 
    427               }
    428               else{
    429                  LEDOff(LED4); 
    430               }
    431          
    432               if (ADC1ConvertedValue[2] > 100){  //Move
    433                 set_ports_PORTOff(Port4);
    434                 LEDOff(LED3);
    435               }
    436               else{
    437                 set_ports_PORTOn(Port4);
    438                  LEDOn(LED3);
    439               }
    440               
    441            vTaskDelay(100);  
    442            } //end for ;;
    443          }
